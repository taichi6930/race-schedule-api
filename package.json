{
    "name": "cdk-race-schedule-app",
    "version": "0.1.0",
    "bin": {
        "cdk-race-schedule-app": "bin/cdk-race-schedule-app.js"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "tsc",
        "watch": "tsc -w",
        "s3-data-update": "aws s3 cp s3://race-schedule-bucket/ ./lib/src/gateway/mockData/csv --recursive",
        "db:import": "TZ=jst ts-node lib/scripts/importCsvData.ts",
        "test": "TZ=jst jest --coverage --maxWorkers=5",
        "test:watch": "TZ=jst jest --coverage --maxWorkers=5 --watchAll",
        "test:watch:silent": "TZ=jst jest --coverage --maxWorkers=5 --watchAll --silent",
        "cdk-deploy": "npm install -g aws-cdk && cdk deploy",
        "start": "ts-node lib/src/index.ts",
        "start:local:no_data": "TZ=jst ENV=LOCAL_NO_INIT_DATA ts-node lib/src/index.ts",
        "dev": "TZ=jst nodemon lib/src/index.ts",
        "dev:test": "TZ=jst ENV=TEST nodemon lib/src/index.ts",
        "dev:local": "TZ=jst ENV=LOCAL nodemon lib/src/index.ts",
        "dev:local:no_data": "TZ=jst ENV=LOCAL_NO_INIT_DATA nodemon lib/src/index.ts",
        "dev:local:made_data": "TZ=jst ENV=LOCAL_INIT_MADE_DATA nodemon lib/src/index.ts",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "npx eslint './lib/**/*.ts' './test/**/*.ts'",
        "lint:fix": "npx eslint --fix './lib/**/*.ts' './test/**/*.ts' --ignore-pattern '**/*.d.ts' && prettier --write .",
        "lint:check": "npx eslint --quiet './lib/**/*.ts' './test/**/*.ts'",
        "type-check": "tsc --noEmit",
        "typedoc": "npx typedoc --out ./docs ./lib/src",
        "pre-commit": "pnpm run type-check && pnpm run lint:check && pnpm run format:check",
        "check:sqlite": "TZ=jst ENV=LOCAL ts-node lib/scripts/check-sqlite.ts",
        "sqliteInit": "TZ=jst npx tsx --tsconfig tsconfig.json lib/scripts/sqliteInit.ts",
        "db:init": "pnpm run s3-data-update;TZ=jst npx tsx scripts/init-db.ts",
        "db:setup": "TZ=jst ts-node lib/scripts/sqliteSetting.ts"
    },
    "engines": {
        "node": ">=24.0.0 <25.0.0"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.859.0",
        "@codegenie/serverless-express": "^4.17.0",
        "@types/aws-lambda": "^8.10.152",
        "@types/express": "^5.0.3",
        "aws-cdk-lib": "2.208.0",
        "aws-lambda": "^1.0.7",
        "better-sqlite3": "12.2.0",
        "cheerio": "^1.1.2",
        "constructs": "^10.4.2",
        "csv-parse": "^6.1.0",
        "csv-writer": "^1.6.0",
        "date-fns": "^4.1.0",
        "dotenv": "^17.2.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.3",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "express": "^5.1.0",
        "google-auth-library": "^10.1.0",
        "googleapis": "^154.1.0",
        "jest-html-reporters": "^3.1.7",
        "nodemon": "^3.1.10",
        "only-allow": "^1.2.1",
        "prettier": "^3.6.2",
        "puppeteer": "^24.15.0",
        "reflect-metadata": "^0.2.2",
        "source-map-support": "^0.5.21",
        "swagger-autogen": "^2.23.7",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "ts-jest": "^29.4.1",
        "ts-node": "^10.9.2",
        "tsyringe": "^4.10.0",
        "typedoc": "^0.28.9",
        "zod": "^4.0.14"
    },
    "devDependencies": {
        "@eslint/js": "^9.31.0",
        "@swc/core": "^1.13.3",
        "@swc/jest": "^0.2.39",
        "@types/better-sqlite3": "^7.6.13",
        "@types/jest": "^30.0.0",
        "@types/node": "24.1.0",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "aws-cdk": "2.1023.0",
        "esbuild": "^0.25.8",
        "eslint": "^9.31.0",
        "eslint-plugin-jsdoc": "^52.0.2",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-unicorn": "^60.0.0",
        "eslint-plugin-unused-imports": "^4.1.4",
        "globals": "^16.3.0",
        "husky": "^9.1.7",
        "jest": "^30.0.5",
        "lint-staged": "^16.1.4",
        "typescript": "~5.9.2",
        "typescript-eslint": "^8.38.0"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "pnpm run lint:fix"
        ]
    }
}
