{
    "name": "cdk-race-schedule-app",
    "version": "0.1.0",
    "bin": {
        "cdk-race-schedule-app": "bin/cdk-race-schedule-app.js"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "tsc",
        "watch": "tsc -w",
        "test": "NODE_OPTIONS='--max_old_space_size=4096' TZ=jst jest --coverage --maxWorkers=49%",
        "cdk-deploy": "pnpm install -g aws-cdk & cdk deploy",
        "start": "ts-node lib/src/index.ts",
        "start:ita": "TZ=jst ENV=ITa ts-node lib/src/index.ts",
        "dev": "TZ=jst nodemon lib/src/index.ts",
        "dev:local": "TZ=jst ENV=LOCAL nodemon lib/src/index.ts",
        "dev:ita": "TZ=jst ENV=ITa nodemon lib/src/index.ts",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "eslint '**/*.{ts,tsx}'",
        "lint:fix": "eslint --fix '**/*.{ts,tsx}' && prettier --write .",
        "lint:check": "eslint --quiet '**/*.{ts,tsx}'",
        "prepare": "husky install",
        "type-check": "tsc --noEmit",
        "pre-commit": "pnpm run type-check && pnpm run lint:check && pnpm run format:check"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "tsc --noEmit",
            "eslint --fix",
            "prettier --write"
        ]
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.654.0",
        "@codegenie/serverless-express": "^4.15.0",
        "@types/aws-lambda": "^8.10.145",
        "@types/cheerio": "^0.22.35",
        "@types/express": "^4.17.21",
        "aws-cdk-lib": "2.155.0",
        "aws-lambda": "^1.0.7",
        "cheerio": "^1.0.0",
        "constructs": "^10.3.0",
        "csv-writer": "^1.6.0",
        "date-fns": "^3.6.0",
        "dotenv": "^16.4.5",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "express": "^4.21.0",
        "gaxios": "^6.7.1",
        "google-auth-library": "^9.14.1",
        "googleapis": "^143.0.0",
        "only-allow": "^1.2.1",
        "prettier": "^3.3.3",
        "puppeteer": "^23.9.0",
        "reflect-metadata": "^0.2.2",
        "source-map-support": "^0.5.21",
        "swagger-autogen": "^2.23.7",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "ts-jest": "^29.2.5",
        "tsyringe": "^4.8.0",
        "typedoc": "^0.26.7"
    },
    "devDependencies": {
        "@eslint/js": "^9.10.0",
        "@swc/core": "^1.7.26",
        "@swc/jest": "^0.2.36",
        "@types/jest": "^29.5.13",
        "@types/node": "20.14.9",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.6",
        "aws-cdk": "2.155.0",
        "eslint": "^9.10.0",
        "eslint-plugin-unused-imports": "^4.1.4",
        "globals": "^15.9.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.10",
        "nodemon": "^3.1.7",
        "ts-node": "^10.9.2",
        "typescript": "~5.5.4",
        "typescript-eslint": "^8.6.0"
    }
}
